{
  //Api地址
  "urls": [ "http://*:8085" ],
  //跨域地址
  "corUrls": [ "http://localhost:9999", "http://127.0.0.1:8848", "http://127.0.0.1:5500", "*" ],
  //Swagger文档
  "swagger": true,
  //统一认证授权服务器
  "IdentityServer": {
    //启用
    "enable": false,
    //地址，开发认证地址前往appconfig.Development.json修改
    "url": "#"
  },
  //面向切面编程
  "aop": {
    //事物
    "transaction": true
  },
  //日志
  "log": {
    //操作日志
    "operation": true
  },
  //限流
  "rateLimit": false,
  //验证码
  "varifyCode": {
    //启用
    "enable": false,
    //字体列表
    "fonts": [ "Times New Roman", "Verdana", "Arial", "Gungsuh", "Impact" ]
  }
}
